{
  "name": "spotify-clone-luizalabs-coding-challenge-react-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prebuild": "npm run codegen && npm run lint",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "codegen": "graphql-codegen --config codegen.ts",
    "codegen:watch": "graphql-codegen --config codegen.ts --watch",
    "lint": "eslint . --ext .ts,.tsx --report-unused-disable-directives --max-warnings=0",
    "test": "vitest",
    "test:unit": "vitest run",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:ci": "start-server-and-test dev http://localhost:5173 test:e2e"
  },
  "dependencies": {
    "@dcoutinho96/spotify-clone-luizalabs-coding-challenge-graphql-schema": "^0.2.0",
    "@tanstack/react-query": "^5.85.5",
    "graphql": "^16.11.0",
    "graphql-request": "^7.2.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@graphql-codegen/cli": "^5.0.7",
    "@graphql-codegen/typescript": "^4.1.6",
    "@graphql-codegen/typescript-operations": "^4.6.1",
    "@graphql-codegen/typescript-react-query": "^6.1.1",
    "@playwright/test": "^1.54.2",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react": "^5.0.0",
    "dotenv": "^17.2.1",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "jsdom": "^26.1.0",
    "msw": "^2.10.5",
    "start-server-and-test": "^2.0.13",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.3",
    "vitest": "^3.2.4",
    "whatwg-fetch": "^3.6.20"
  },
  "description": "Spotiy Clone (Luizalabs coding challenge)",
  "main": "eslint.config.js",
  "keywords": [],
  "author": "",
  "license": "ISC"
}
